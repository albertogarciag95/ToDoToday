{
  "name": "todo-today",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "debug": "babel-node --nolazy --inspect-brk=9228 src/node-api/src/index.js",
    "build": "ng build",
    "test": "ng test --watch --code-coverage && npm run e2e && npm run lint",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4201",
    "server-clean": "rimraf dist",
    "db": "babel-node src/node-api/db/index.js",
    "server-build": "npm run server-clean && cd src/node-api && babel src -d dist",
    "server-dev": "npm run db && cd src/node-api/src && nodemon --exec babel-node index.js",
    "server-pro": "npm run server-build && npm run db && cd src/node-api/dist && node index.js",
    "server-test": "cd src/node-api && nyc --reporter=html --reporter=text mocha --require @babel/register --recursive test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "geolib": "^3.2.1",
    "https": "^1.0.0",
    "mapbox-gl": "^1.10.0",
    "mocha": "^7.1.1",
    "mongoose": "^5.9.7",
    "node-cron": "^2.0.3",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "@types/chai": "^4.2.11",
    "@types/chai-spies": "^1.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/mapbox-gl": "^1.9.1",
    "@types/node": "^12.11.1",
    "babel-plugin-istanbul": "^6.0.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.1",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
